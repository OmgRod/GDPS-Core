datasource gdps {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model AccComments {
  commentID Int     @id @map("commentID") @default(autoincrement())
  userID    Int     @map("userID")
  userName  String  @map("userName")
  comment   String  @map("comment")
  secret    String  @default("unused") @map("secret")
  timestamp Int     @map("timestamp")
  likes     Int     @default(0) @map("likes")
  isSpam    Boolean @default(false) @map("isSpam")

  @@map("acccomments")
}

model Account {
  accountID      Int     @id @map("accountID") @default(autoincrement())
  userName       String  @map("userName")
  password       String  @map("password")
  gjp2           String? @map("gjp2")
  email          String  @map("email")
  isAdmin        Int     @default(0) @map("isAdmin")
  mS             Int     @default(0) @map("mS")
  frS            Int     @default(0) @map("frS")
  cS             Int     @default(0) @map("cS")
  youtubeurl     String  @default("") @map("youtubeurl")
  twitter        String  @default("") @map("twitter")
  twitch         String  @default("") @map("twitch")
  salt           String  @default("") @map("salt")
  registerDate   Int     @default(0) @map("registerDate")
  friendsCount   Int     @default(0) @map("friendsCount")
  discordID      BigInt  @default(0) @map("discordID")
  discordLinkReq BigInt  @default(0) @map("discordLinkReq")
  isActive       Boolean @default(false) @map("isActive")

  @@map("accounts")
}

model Actions {
  ID        Int    @id @default(autoincrement()) @map("ID")
  type      Int    @default(0)
  value     String @default("0")
  timestamp Int    @default(0)
  value2    String @default("0")
  value3    Int    @default(0)
  value4    Int    @default(0)
  value5    Int    @default(0)
  value6    Int    @default(0)
  account   Int    @default(0)

  @@map("actions")
}

model ActionsDownloads {
  id         Int      @id @default(autoincrement()) @map("id")
  levelID    Int      @map("levelID")
  ip         Bytes?   @map("ip")
  uploadDate DateTime @default(now()) @map("uploadDate")

  @@map("actions_downloads")
}

model ActionsLikes {
  id         Int      @id @default(autoincrement()) @map("id")
  itemID     Int      @map("itemID")
  type       Int      @default(0) @map("type")
  isLike     Boolean  @default(false) @map("isLike")
  ip         Bytes?   @map("ip")
  uploadDate DateTime @default(now()) @map("uploadDate")

  @@map("actions_likes")
}

model BannedIPs {
  ID Int    @id @default(autoincrement()) @map("ID")
  IP String @default("127.0.0.1") @map("IP")

  @@map("bannedips")
}

model Blocks {
  ID      Int @id @default(autoincrement()) @map("ID")
  person1 Int @map("person1")
  person2 Int @map("person2")

  @@map("blocks")
}

model Comments {
  commentID Int     @id @default(autoincrement()) @map("commentID")
  userID    Int     @map("userID")
  userName  String  @map("userName")
  comment   String  @map("comment")
  secret    String  @default("none") @map("secret")
  levelID   Int     @map("levelID")
  timestamp Int     @map("timestamp")
  likes     Int     @default(0) @map("likes")
  percent   Int     @default(0) @map("percent")
  isSpam    Boolean @default(false) @map("isSpam")

  @@map("comments")
}

model CpShares {
  shareID Int @id @default(autoincrement()) @map("shareID")
  levelID Int @map("levelID")
  userID  Int @map("userID")

  @@map("cpshares")
}

model DailyFeatures {
  feaID     Int @id @default(autoincrement()) @map("feaID")
  levelID   Int @map("levelID")
  timestamp Int @map("timestamp")
  type      Int @default(0) @map("type")

  @@map("dailyfeatures")
}

model FriendReqs {
  ID          Int    @id @default(autoincrement()) @map("ID")
  accountID   Int    @map("accountID")
  toAccountID Int    @map("toAccountID")
  comment     String @map("comment")
  uploadDate  Int    @map("uploadDate")
  isNew       Boolean @default(true) @map("isNew")

  @@map("friendreqs")
}

model Friendships {
  ID       Int @id @default(autoincrement()) @map("ID")
  person1  Int @map("person1")
  person2  Int @map("person2")
  isNew1   Int @map("isNew1")
  isNew2   Int @map("isNew2")

  @@map("friendships")
}

model Gauntlets {
  ID     Int @id @default(autoincrement()) @map("ID")
  level1 Int @map("level1")
  level2 Int @map("level2")
  level3 Int @map("level3")
  level4 Int @map("level4")
  level5 Int @map("level5")

  @@map("gauntlets")
}

model Levels {
  levelID         Int      @id @default(autoincrement()) @map("levelID")
  gameVersion     Int      @map("gameVersion")
  binaryVersion   Int      @default(0) @map("binaryVersion")
  userName        String   @map("userName")
  levelName       String   @map("levelName")
  levelDesc       String   @map("levelDesc")
  levelVersion    Int      @map("levelVersion")
  levelLength     Int      @default(0) @map("levelLength")
  audioTrack      Int      @map("audioTrack")
  auto            Int      @map("auto")
  password        Int      @map("password")
  original        Int      @map("original")
  twoPlayer       Int      @default(0) @map("twoPlayer")
  songID          Int      @default(0) @map("songID")
  songIDs         String   @default("") @map("songIDs")
  sfxIDs          String   @default("") @map("sfxIDs")
  objects         Int      @default(0) @map("objects")
  coins           Int      @default(0) @map("coins")
  requestedStars  Int      @default(0) @map("requestedStars")
  extraString     String   @map("extraString")
  levelString     String?  @map("levelString")
  levelInfo       String   @map("levelInfo")
  secret          String   @map("secret")
  starDifficulty  Int      @default(0) @map("starDifficulty")
  downloads       Int      @default(300) @map("downloads")
  likes           Int      @default(100) @map("likes")
  starDemon       Int      @map("starDemon")
  starAuto        Boolean  @default(false) @map("starAuto")
  starStars       Int      @default(0) @map("starStars")
  uploadDate      BigInt   @map("uploadDate")
  updateDate      BigInt   @map("updateDate")
  rateDate        BigInt   @default(0) @map("rateDate")
  starCoins       Int      @default(0) @map("starCoins")
  starFeatured    Int      @default(0) @map("starFeatured")
  starHall        Int      @default(0) @map("starHall")
  starEpic        Int      @default(0) @map("starEpic")
  starDemonDiff   Int      @default(0) @map("starDemonDiff")
  userID          Int      @map("userID")
  extID           String   @map("extID")
  unlisted        Int      @map("unlisted")
  originalReup    Int      @default(0) @map("originalReup")
  hostname        String   @map("hostname")
  isCPShared      Int      @default(0) @map("isCPShared")
  isDeleted       Int      @default(0) @map("isDeleted")
  isLDM           Int      @default(0) @map("isLDM")
  unlisted2       Int      @default(0) @map("unlisted2")
  wt              Int      @default(0) @map("wt")
  wt2             Int      @default(0) @map("wt2")
  ts              Int      @default(0) @map("ts")
  settingsString  String   @default("") @map("settingsString")

  @@map("levels")
}

model LevelScores {
  scoreID   Int    @id @default(autoincrement()) @map("scoreID")
  accountID Int    @map("accountID")
  levelID   Int    @map("levelID")
  percent   Int    @map("percent")
  uploadDate Int   @map("uploadDate")
  attempts  Int    @default(0) @map("attempts")
  coins     Int    @default(0) @map("coins")
  clicks    Int    @default(0) @map("clicks")
  time      Int    @default(0) @map("time")
  progresses String @map("progresses")
  dailyID   Int    @default(0) @map("dailyID")

  @@map("levelscores")
}

model Links {
  ID             Int    @id @default(autoincrement()) @map("ID")
  accountID      Int    @map("accountID")
  targetAccountID Int   @map("targetAccountID")
  server         String @map("server")
  timestamp      Int    @map("timestamp")
  userID         Int    @map("userID")
  targetUserID   Int    @map("targetUserID")

  @@map("links")
}

model Lists {
  listID         Int    @id @default(autoincrement()) @map("listID")
  listName       String @map("listName")
  listDesc       String @map("listDesc")
  listVersion    Int    @map("listVersion")
  accountID      Int    @map("accountID")
  downloads      Int    @default(0) @map("downloads")
  starDifficulty Int    @map("starDifficulty")
  likes          Int    @default(0) @map("likes")
  starFeatured   Int    @default(0) @map("starFeatured")
  starStars      Int    @default(0) @map("starStars")
  listlevels     String @map("listlevels")
  countForReward Int    @default(0) @map("countForReward")
  uploadDate     Int    @default(0) @map("uploadDate")
  updateDate     Int    @default(0) @map("updateDate")
  original       Int    @default(0) @map("original")
  unlisted       Int    @default(0) @map("unlisted")

  @@map("lists")
}

model MapPacks {
  ID        Int    @id @default(autoincrement()) @map("ID")
  name      String @map("name")
  levels    String @map("levels")
  stars     Int    @map("stars")
  coins     Int    @map("coins")
  difficulty Int   @map("difficulty")
  rgbcolors String @map("rgbcolors")
  colors2   String @default("none") @map("colors2")

  @@map("mappacks")
}

model Messages {
  messageID   Int    @id @default(autoincrement()) @map("messageID")
  userID      Int    @map("userID")
  userName    String @map("userName")
  body        String @map("body")
  subject     String @map("subject")
  accID       Int    @map("accID")
  toAccountID Int    @map("toAccountID")
  timestamp   Int    @map("timestamp")
  secret      String @default("unused") @map("secret")
  isNew       Int    @default(0) @map("isNew")

  @@map("messages")
}

model ModActions {
  ID      Int    @id @default(autoincrement()) @map("ID")
  type    Int    @default(0) @map("type")
  value   String @default("0") @map("value")
  timestamp Int  @default(0) @map("timestamp")
  value2  String @default("0") @map("value2")
  value3  Int    @default(0) @map("value3")
  value4  String @default("0") @map("value4")
  value5  Int    @default(0) @map("value5")
  value6  Int    @default(0) @map("value6")
  account Int    @default(0) @map("account")
  value7  String @default("0") @map("value7")

  @@map("modactions")
}

model ModIpPerms {
  categoryID    Int @id @default(autoincrement()) @map("categoryID")
  actionFreeCopy Int @map("actionFreeCopy")

  @@map("modipperms")
}

model ModIps {
  ID            Int    @id @default(autoincrement()) @map("ID")
  IP            String @map("IP")
  isMod         Int    @map("isMod")
  accountID     Int    @map("accountID")
  modipCategory Int    @map("modipCategory")

  @@map("modips")
}

model PlatScores {
  ID        Int @id @default(autoincrement()) @map("ID")
  accountID Int @default(0) @map("accountID")
  levelID   Int @default(0) @map("levelID")
  time      Int @default(0) @map("time")
  points    Int @default(0) @map("points")
  timestamp Int @map("timestamp")

  @@map("platscores")
}

model Quests {
  ID     Int    @id @default(autoincrement()) @map("ID")
  type   Int    @map("type")
  amount Int    @map("amount")
  reward Int    @map("reward")
  name   String @map("name")

  @@map("quests")
}

model Reports {
  ID       Int    @id @default(autoincrement()) @map("ID")
  levelID  Int    @map("levelID")
  hostname String @map("hostname")

  @@map("reports")
}

model RoleAssign {
  assignID BigInt @id @default(autoincrement()) @map("assignID")
  roleID   BigInt @map("roleID")
  accountID BigInt @map("accountID")

  @@map("roleassign")
}

model Roles {
  roleID        BigInt @id @default(autoincrement()) @map("roleID")
  priority      Int    @default(0) @map("priority")
  roleName      String @map("roleName")
  commandRate   Int    @default(0) @map("commandRate")
  commandFeature Int   @default(0) @map("commandFeature")
  commandEpic   Int    @default(0) @map("commandEpic")
  commandUnepic Int    @default(0) @map("commandUnepic")
  commandVerifycoins Int @default(0) @map("commandVerifycoins")
  commandDaily  Int    @default(0) @map("commandDaily")
  commandWeekly Int    @default(0) @map("commandWeekly")
  commandDelete Int    @default(0) @map("commandDelete")
  commandSetacc Int    @default(0) @map("commandSetacc")
  commandRenameOwn Int @default(1) @map("commandRenameOwn")
  commandRenameAll Int @default(0) @map("commandRenameAll")
  commandPassOwn Int @default(1) @map("commandPassOwn")
  commandPassAll Int @default(0) @map("commandPassAll")
  commandDescriptionOwn Int @default(1) @map("commandDescriptionOwn")
  commandDescriptionAll Int @default(0) @map("commandDescriptionAll")
  commandPublicOwn Int @default(1) @map("commandPublicOwn")
  commandPublicAll Int @default(0) @map("commandPublicAll")
  commandUnlistOwn Int @default(1) @map("commandUnlistOwn")
  commandUnlistAll Int @default(0) @map("commandUnlistAll")
  commandSharecpOwn Int @default(1) @map("commandSharecpOwn")
  commandSharecpAll Int @default(0) @map("commandSharecpAll")
  commandSongOwn Int @default(1) @map("commandSongOwn")
  commandSongAll Int @default(0) @map("commandSongAll")
  profilecommandDiscord Int @default(1) @map("profilecommandDiscord")
  actionRateDemon Int @default(0) @map("actionRateDemon")
  actionRateStars Int @default(0) @map("actionRateStars")
  actionRateDifficulty Int @default(0) @map("actionRateDifficulty")
  actionRequestMod Int @default(0) @map("actionRequestMod")
  actionSuggestRating Int @default(0) @map("actionSuggestRating")
  actionDeleteComment Int @default(0) @map("actionDeleteComment")
  toolLeaderboardsban Int @default(0) @map("toolLeaderboardsban")
  toolPackcreate Int @default(0) @map("toolPackcreate")
  toolQuestsCreate Int @default(0) @map("toolQuestsCreate")
  toolModactions Int @default(0) @map("toolModactions")
  toolSuggestlist Int @default(0) @map("toolSuggestlist")
  dashboardModTools Int @default(0) @map("dashboardModTools")
  modipCategory Int @default(0) @map("modipCategory")
  isDefault Int @default(0) @map("isDefault")
  commentColor String @default("000,000,000") @map("commentColor")
  modBadgeLevel Int @default(0) @map("modBadgeLevel")

  @@map("roles")
}

model Songs {
  ID          Int    @id @default(autoincrement()) @map("ID")
  name        String @map("name")
  authorID    Int    @map("authorID")
  authorName  String @map("authorName")
  size        String @map("size")
  download    String @map("download")
  hash        String @default("") @map("hash")
  isDisabled  Int    @default(0) @map("isDisabled")
  levelsCount Int    @default(0) @map("levelsCount")
  reuploadTime Int   @default(0) @map("reuploadTime")

  @@map("songs")
}

model Suggest {
  ID              Int @id @default(autoincrement()) @map("ID")
  suggestBy       Int @default(0) @map("suggestBy")
  suggestLevelId  Int @default(0) @map("suggestLevelId")
  suggestDifficulty Int @default(0) @map("suggestDifficulty")
  suggestStars    Int @default(0) @map("suggestStars")
  suggestFeatured Int @default(0) @map("suggestFeatured")
  suggestAuto     Int @default(0) @map("suggestAuto")
  suggestDemon    Int @default(0) @map("suggestDemon")
  timestamp       Int @map("timestamp")

  @@map("suggest")
}

model Users {
  userID        Int     @id @default(autoincrement()) @map("userID")
  isRegistered  Int     @map("isRegistered")
  extID         String  @map("extID")
  userName      String  @default("undefined") @map("userName")
  stars         Int     @default(0) @map("stars")
  demons        Int     @default(0) @map("demons")
  icon          Int     @default(0) @map("icon")
  color1        Int     @default(0) @map("color1")
  color2        Int     @default(3) @map("color2")
  color3        Int     @default(0) @map("color3")
  iconType      Int     @default(0) @map("iconType")
  coins         Int     @default(0) @map("coins")
  userCoins     Int     @default(0) @map("userCoins")
  special       Int     @default(0) @map("special")
  gameVersion   Int     @default(0) @map("gameVersion")
  secret        String  @default("none") @map("secret")
  accIcon       Int     @default(0) @map("accIcon")
  accShip       Int     @default(0) @map("accShip")
  accBall       Int     @default(0) @map("accBall")
  accBird       Int     @default(0) @map("accBird")
  accDart       Int     @default(0) @map("accDart")
  accRobot      Int?    @default(0) @map("accRobot")
  accGlow       Int     @default(0) @map("accGlow")
  accSwing      Int     @default(0) @map("accSwing")
  accJetpack    Int     @default(0) @map("accJetpack")
  dinfo         String  @default("") @map("dinfo")
  sinfo         String  @default("") @map("sinfo")
  pinfo         String  @default("") @map("pinfo")
  creatorPoints Float   @default(0) @map("creatorPoints")
  IP            String  @default("127.0.0.1") @map("IP")
  lastPlayed    Int     @default(0) @map("lastPlayed")
  diamonds      Int     @default(0) @map("diamonds")
  moons         Int     @default(0) @map("moons")
  orbs          Int     @default(0) @map("orbs")
  completedLvls Int     @default(0) @map("completedLvls")
  accSpider     Int     @default(0) @map("accSpider")
  accExplosion  Int     @default(0) @map("accExplosion")
  chest1time    Int     @default(0) @map("chest1time")
  chest2time    Int     @default(0) @map("chest2time")
  chest1count   Int     @default(0) @map("chest1count")
  chest2count   Int     @default(0) @map("chest2count")
  isBanned      Int     @default(0) @map("isBanned")
  isCreatorBanned Int   @default(0) @map("isCreatorBanned")

  @@map("users")
}



